@import '../reset'
@import 'url'
//opacity_support------------------------------------------------------------------------------------------
support-for-ie ?= true

opacity(n)
    opacity n
    if support-for-ie
        filter unquote('progid DXImageTransform.Microsoft.Alpha(Opacity=' + round(n * 100) + ')')
//------------------------------------------------------------------------------------------
body
    margin 0 
    color #00bbfd 
    font 12px/20px "宋体"
    background #0c1220
a
    outline  none 
    color #00bbfd
    &:hover
        color #fff 
p 
    margin 0 
    text-indent  2em 
.container 
    min-width  980px 
    height 1593px 
.container_nyz
    background url($bg_ynz) no-repeat top center
.container_jws
    background url($bg_jws) no-repeat top center
.container_qxs
    background url($bg_qxs) no-repeat top center
.container_gdj
    background url($bg_gdj) no-repeat top center

.main 
    width 980px
    margin 0 auto
    height 100%
    position relative
#nav 
    width 664px
    height 62px
    position absolute
    left 285px 
.logo 
    width 150px  
    height 55px 
    position absolute 
    left 50px 
    top 10px 
    text-indent  -9999px  
.profession,.profession_content 
    width 512px 
    height 124px 
    position absolute 
    left 390px 
    top 300px  
.ico 
    width 124px  
    margin-right  4px 
    height 100% 
    display  inline-block 
    float  left 
    text-indent  -9999px 
    background url($btn) no-repeat  
.ico_gdj 
    background-position -5px -4px
    &:hover 
        background-position -5px -132px
.check_gdj 
    background-position -5px -260px 
     
.ico_jws 
    background-position -133px -4px
