        link(href='/stylesheets/index/main.css',rel='stylesheet')
        script(src="http://s1.syyx.com/plugins/jquery/jquery-1.7.1.min.js")
        script(src='/javascripts/index/out.js')
    body
        .container_out
            .container
                .content
                    .flash_banner
                        #flash_banner
                    a.logo_link(href='#{nysj_main}')
                    include ../include/nav
                    .top_video
                        #top_video_container
                        a.open_top_dialog(href='#')
                    .fn_group
                        a(href='#',class='fn_download_btn',target='_blank')
                        a(href='#',class='fn_active_btn',target='_blank')
                        a(href='#{nav_pay}',class='fn_charge_btn',target='_blank')
                    #tabs_picture_news
                        ul
                    #game_hot
                        a.more(href='/news_list.html', target='_blank') 更多>>
                        .top_news
                            a.thumb(href='#',target='_blank')
                                img(src='')
                            h1
                                a(href='#',target='_blank')
                                span
                            .summary
                            a.view_all(href='#', target='_blank') 查看全文>>
                        ul.text_list
                        ul.img_list
                    a.down_load_mini_client(href='#{download_mini}')
                    a.get_active_code(href='#{activate}',target='_blank')
                    #tabs_role
                        ul
                            li
                                a.yineng(href='#yineng')
                            li
                                a.qiangxie(href='#qiangxie')
                            li
                                a.jianwu(href='#jianwu')
                            li
                                a.gedou(href='#gedou')
                        #yineng
                            .video_container
                                #yineng_video
                            .jineng_name
                                em(title="乱雷穿空：对自身一定范围内的可攻击目标造成群体伤害。")
                                em(title="漫天惊雷：异能者雷电技能，对目标及目标周围的敌人造成伤害。")
                                em(title="闪电诱爆：提高攻击暴击几率。")
                                em(title="能量护盾：获得技能效果后，用精神值抵消部分的所受伤害。")
                                em(title="怒雷闪：异能者高级攻击技能，一定几率对攻击目标周围的敌对目标进行额外攻击。")
                                em(title="瞬间移动：瞬间移动到指定地点，以追击或躲避。")
                                em(title="天雷陨：使用怒雷闪时有一定几率召唤一个从天而降的巨大陨石对目标造成伤害，并在目标当前位置形成持续掉血的灼烧区域。")
                            a.more(href='#',target='_blank')
                        #qiangxie
                            .video_container
                                #qiangxie_video
                            .jineng_name
                                em(title="高效杀伤：BUFF技能，增加自身以及一定范围内队友的攻击伤害。")
                                em(title="精确打击：花一定时间瞄准，对目标造成非常大的伤害，射程极远。")
                                em(title="连射：枪械师中级攻击技能，10%几率触发四连击。")
                                em(title="扫射：枪械师中级技能，对目标及目标周围的敌人造成伤害。")
                                em(title="毒雾陷阱：放置一个陷阱，被敌方触发后，在陷阱区域内的敌对目标将获得减速及受到伤害加深的效果")
                                em(title="隐身：进入隐身状态一段时间，隐身期间移动速度加快。")
                                em(title="专注射击：BUFF技能，提高自身的命中与忽闪，减少自身的闪避。")
                            a.more(href='#',target='_blank')
                        #jianwu
                            .video_container
                                #jianwu_video
                            .jineng_name
                                em(title="剑术精通：提高剑武士物理攻击。")
                                em(title="破甲：攻击出现连斩时，降低目标防御一定时间。")
                                em(title="焚心斩：消耗一定生命值与精神值对目标造成巨大伤害。")
                                em(title="剑武战意：全面提升剑武士属性。")
                                em(title="掠影：冲向目标并对目标造成一定伤害。")
                                em(title="嗜血：使用极光刃影时有一定几率将造成的伤害按比例在2秒内转化为自己的生命值。")
                                em(title="死里逃生：当剑武士受到将会导致死亡的伤害时，将有一定几率触发此技能来忽略此伤害，触发间隔30秒。")
                            a.more(href='#',target='_blank')
                        #gedou
                            .video_container
                                #gedou_video
                            .jineng_name
                                em(title="风刃乱舞：格斗家群体技能，攻击自身周围目标。")
                                em(title="复仇魂爆：使用技能后，持续时间结束造成范围伤害。持续时间内受到的伤害如果超过一定值，格斗家会按一定比例对附近目标造成巨大的反弹伤害。")
                                em(title="减速：一段时间内减少对方移动速度。")
                                em(title="怒意强袭：格斗家中级攻击技能，几率触发攻击加速。")
                                em(title="破敌：攻击加速被触发时，增加暴击值。")
                                em(title="嘲讽：咒骂目标及目标周边单位，强行让目标攻击自己。")
                                em(title="灵魂链接：BUFF技能，格斗家可以分担目标单位的伤害，自身生命值低于30%时，断开锁链。")
                            a.more(href='#',target='_blank')
                    #screenshot
                        a.pre(href='#')
                        a.next(href='#')
                        .screenshot_list_outer
                            .screenshot_list
                    .service_info
                        div 客服电话：400-166-8666
                        div 客服传真: 0755-61864520
                        div 客服邮箱：kf_nycs@syyx.com
                        a.login_service_center(href='http://service.syyx.com/index_time.aspx', target='_blank') >> 进入客服中心
                    #download_panel
                        a.close(href='#')
                        .mini
                            a.download_btn_mini(href='#{download_mini}')
                            .des 适合新用户使用
                            span.size
                            span.version
                            .md5
                        .full
                            a.download_btn_full(href='#{download_full}')
                            .des 适合所有用户使用
                            span.size
                            span.version
                            .md5
                        .get_active_code_in_download_panel
                            a(href='#{activate}', target='_blank') 还没有激活码？快来抢！
                    #download_panel_not_open
                        a.close(href='#')
                    #activate_form
                        a.close(href='#')
                        form(action='#')
                            .form_item
                                span 激活码：
                                input(type='text', name='code',maxlength='30')
                            .reg_tip
                                a(href='#{activate}', target='_blank') 没有激活码？马上去抢！
                            .form_item
                                span 游戏账号：
                                input(type='text', name='account',maxlength='16')
                            .reg_tip 没有账号？请在右侧快速注册！
                            .activate_form_submit_btn
                                a(href='#')
                                span
                    #activate_result_1
                        a.close(href='#')
                        h3 激活成功
                        p
                            | 您的账号：
                            span
                            | 已成功激活，请下载游戏客户端。
                        p 9月15日首次删档封测，有你更精彩！
                        a.download_mini(href='#{download_mini}')
                    #activate_result_2
                        a.close(href='#')
                        h3 激活失败
                        p
                        a#activate_failure_btn(href='#{nysj_main}')
                    #reg
                        #reg_step1
                            table.reg_form
                                tr
                                    td.reg_form_item_name 账号：
                                    td.reg_form_item_input
                                        input#txtUserAccount(type="text",maxlength="16")
                                    td
                                        #txtUserAccountTip
                                tr
                                    td.reg_form_item_name 密码：
                                    td.reg_form_item_input
                                        input#txtPasswords(type='password',maxlength="16")
                                        input#txtPasswordsVal.disnone(type="text")
                                    td
                                        #txtPasswordsTip
                                tr
                                    td.reg_form_item_name 重复密码：
                                    td.reg_form_item_input
                                        input#txtConfirm(type="password")
                                        input#txtConfirmVal.disnone(type="text")
                                    td
                                        #txtConfirmTip
                                tr
                                    td.reg_form_item_name 真实姓名：
                                    td.reg_form_item_input
                                        input#txtTrueName(type='text',maxlength="20")
                                    td
                                        #txtTrueNameTip
                                tr
                                    td.reg_form_item_name 身份证：
                                    td.reg_form_item_input
                                        input#txtCardID(type="text",maxlength="18")
                                    td
                                        #txtCardIDTip
                                tr
                                    td.reg_form_item_name 验证码：
                                    td.reg_form_item_input.validate_num_input
                                        input#txtValidate(type='text',maxlength='20')
                                        img#validateimg.disnone(src='',alt='点击刷新验证码')
                                    td
                                        #txtValidateVal
                            .agreement
                                input#chk_XieYi(type="checkbox",checked)
                                | 同意
                                a(href='#{policy}',target="_blank")
                                    《使用协议及隐私协议》
                            .reg_submit
                                a#reg_button(href="#",hidefocus="true")
                                span#regProcessTip
                        #reg_step2
                            span.success_icon
                            span.success_text 注册成功!
                            span.success_account 账号：
                                span#reg_account1 asdrf
                            .get_active_code_text_after_reg
                                h3 领取激活码：
                                p 您可以通过下方按钮获取《诺亚传说》时间版首次删档封测测试资格。
                            a.get_code_link(href='#{activate}', target='_blank')
                            .download_text_after_reg 建议您下载：
                            a.down_load_mini_client_after_reg(href='#{download_mini}')
                    .disnone
                        #top_video_detail
                            #top_video_detail_container
                    include ../include/copyright
        include ../include/cnzz