div#content
	div#search
		table
			tr
				td	游戏
					select#ddlGame
				td	日期
					input#txtStartTime(type="text",class="easyui-datebox")
					到
					input#txtEndTime(type="text",class="easyui-datebox")
				td  媒体类型
					select#ddlClass
				td	媒体
					select#ddlMedia
				td  广告
					select#ddlAd
				td  子站
					select#ddlAdWebNumber				
				td  
					&nbsp
					a#btnSearch(href="javascript:void(0)",class="easyui-linkbutton")	查询
					&nbsp
					a#btnExcel(href="javascript:void(0)",class="easyui-linkbutton")	导出Excel
		table
			tr
				td
					a#a_today(href="javascript:void(0)")[今天]
					a#a_yesterday(href="javascript:void(0)")[昨天]
					a#a_pre10days(href="javascript:void(0)")[最近10天]
					a#a_pre20days(href="javascript:void(0)")[最近20天]
					a#a_pre1month(href="javascript:void(0)")[最近一个月]
				td
					input#radDay(type="radio",name="rdtype1", value="0",checked="checked")	
					按日

					input#radTotal(type="radio",name="rdtype1",value="1")	
					合计
				td
					input#radSchedule(type="radio",name="rdtype2",value="1")	
					只看排期
					input#radAll(type="radio",name="rdtype2",checked="checked",value="0")
					所有

