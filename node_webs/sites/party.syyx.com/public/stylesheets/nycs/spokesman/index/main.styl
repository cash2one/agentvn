body
    margin 0
    background #0f002b
    color #9ea39f
    font-size 12px
    line-height 22px
.container
    height 1920px
    min-width 980px
    overflow hidden
    font-size 12px
.bg_nav
    height 620px
    background url($bg_nav) no-repeat top center
.bg_content
    height 940px
    background url($bg_content) no-repeat top center
.bg_bottom
    height 440px
    background url($bg_bottom) no-repeat top center
.main
    width 1030px
    margin 0 auto
    position relative
    height 100%
    overflow hidden
.nav
    text-align right
    line-height 30px
    color #c182fc
    top 23px
    left 605px
    position absolute
    a
        color #c182fc
        margin 16px
        font-size 12px
        text-decoration none
        &:hover
            color #fff
            text-decoration underline 
#a_nycs
    top 20px
    left 40px
