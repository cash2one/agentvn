$path        = "http://r.syyx.com/events/p032/"
$bg          = $path"bg.jpg"
$main        = $path"main.jpg"
$btn         = $path"btn.jpg"
$msg_bg      = $path'msg_bg.png'
$time_bg     = $path'time_bg.png'
$loading     = $path'loading.gif'
$gdj         = $path'gdj.jpg'
$jws         = $path'jws.jpg'
$qxs         = $path'qxs.jpg'
$ynz         = $path'ynz.jpg'
$close       = $path'close.jpg'
$reg_scroll  = $path'reg_scroll.jpg'
$reg_bg      = $path'reg_bg.jpg'
$reg_btn     = $path'reg_btn.jpg'
$loading     = $path'loading.gif'
$right_wrong = $path'right_wrong.png'
$loader      = $path'ajax-loader.gif'


body
    margin 0
    font 12px/2em "宋体"
    color #dba66e
    background rgb(34,28,16)
a
    text-decoration none
    color #d9b070
    outline none
    &:hover
        color #fff
img
    border none
.container
    background url($bg) no-repeat top center
    min-width 980px
.wrap
    background url($main) no-repeat center 540px
    position relative
.main
    width 980px
    height 1424px
    position relative
    margin 0 auto
.nav
    text-align right
    line-height 30px
    color #d9b070
    padding-right 120px
.logo
    position absolute
    width 187px
    height 86px
    left 7px
    text-indent -9999cm
#prize
    width 250px
    height 72px
    position absolute
    top 333px
    left 270px
#flash, #flash_but, #flash_btn
    width 186px
    height 105px
    position absolute
    top 333px
    left 35px
#flash_btn
    background url($btn) no-repeat 999cm
    cursor pointer
.mini
    width 165px
    position absolute
    height 35px
    left 46px
    top 460px
    text-indent -9999cm
    background url($btn) no-repeat 9999cm
    &:hover
        background-position -8px -67px
#to_reg_flash,#to_reg_flash2,#to_reg_1
    width 221px
    height 72px
    position absolute
    top 421px
    left 280px
#to_reg_flash2
    visibility hidden
#to_reg_1
    background url($btn) no-repeat 999cm
    text-indent -9999cm
#img_focus
    width 680px
    height 400px
    position absolute
    top 581px
    left 4px
    background url($loading) center 200px no-repeat
#img_focus_tab
    height 44px
    a
        width 169px
        display inline-block
        float left
        height 42px
        cursor pointer
        background url($btn) no-repeat 999px 999px
    .check_0
        background-position -8px -14px
    .check_1
        background-position -177px -14px
    .check_2
        background-position -347px -14px
    .check_3
        background-position -516px -14px
#img_focus_img
    height 354px
    img
        display none
        position absolute
.comment
    position absolute
    width 280px
    height 300px
    overflow hidden
    right 2px
    line-height 20px
    top 550px
    ul
        padding 0
        margin 0
        height 1500px
        li
            list-style none
            height 60px
            float left
            width 280px
            img
                float left
                margin-top 7px
            p
                margin 5px 0 0 0
                float left
                width 132px
                height 40px
                padding 5px 10px 4px 20px
                color #1a2626
                background url($msg_bg) no-repeat 5px 0
                overflow hidden
            span
                float left
                display block
                width 65px
                height 20px
                margin-top 20px
                background url($time_bg) no-repeat
                text-align center
                color #fff
#job
    position absolute
    top 988px
    left 0
    width 683px
    height 261px
    #j_tab
        height 38px
        position absolute
        width 476px
        left 129px
        top 4px
        span
            display block
            width 96px
            float left
            background url($btn) no-repeat 9999cm
            height 100%
            cursor pointer
        .check_0
            background-position -205px -66px
        .check_1
            background-position -301px -66px
        .check_2
            background-position -397px -66px
        .check_3
            background-position -493px -66px
            width 95px
    #j_con
        height 273px
        width 100%
        top 0px
        position absolute
        overflow hidden
        line-height 25px
        .con
            width 100%
            height 100%
            position absolute
            top 0
            display none
        .jws
            background url($jws) no-repeat right top
        .qxs
            background url($qxs) no-repeat right top
        .ynz
            background url($ynz) no-repeat right top
        .gdj
            background url($gdj) no-repeat right top
        .feat
            width 200px
            position absolute
            top 100px
            left 320px
        .video
            width 238px
            height 134px
            position absolute
            top 105px
            left 30px
            background #000
            overflow hidden
            border 1px solid #8d5934
        #to_reg_3
            width 99px
            height 24px
            background url($btn) no-repeat -10px -116px
            position absolute
            top 160px
            right 60px
            cursor pointer
            &:hover
                background-position -10px -140px
    #to_reg_3
        width 99px
        height 24px
        position absolute
        background url($btn) no-repeat -8px -115px
        top 217px
        left 330px
        &:hover
            background-position  -8px -144px
#img_scroll
    width 263px
    height 217px
    position absolute
    top 1035px
    right 10px
    .button
        width 14px
        height 58px
        float left
        cursor pointer
        margin-top 75px
    .scroll_box
        overflow hidden
        width 235px
        text-align center
        float left
        height 100%
        #scroll_body
            width 1410px
        a
            width 235px
            float left
            display block
            height 217px
            background url($loading) no-repeat center
        img
            display none
            border 1px solid #8d5934
.copy_right
    width 980px
    text-align center
    position absolute
    top 1310px
    line-height 18px
    color rgb(231,229,170)
    .feet_txt
        text-align left
        margin 10px 0 0 380px
    .feet_logo
        position absolute
        width 140px
        height 60px
        left 225px
        top 30px
.pop_video_detail
    background: #54200A;
    border 1px solid #873410
#pop_flash
    border 1px solid #873410
    background #000
    line-height 0
    height 350px
.ui-dialog .ui-dialog-titlebar-close
    background url($close) no-repeat
object
    outline none
#reg_scroll
    width 70px
    height 383px
    position absolute
    top 100px
    right 0
    #reg_but
        width 70px
        height 273px
        background url($reg_scroll) no-repeat 0 0
        position absolute
        &:hover
            background-position -70px 0
    #reg_box
        width 266px
        height 383px
        display none
        background url($reg_bg) no-repeat
    .reg_panel
        width 230px
        padding 50px 0 0 18px
        color #c998ff
        table
            height 162px
            td
                height 27px
                width 20px
            input
                background #776595
                border 1px solid #683ec8
                height 18px
                color #221334
                line-height 18px
                width 145px
            .focus_in_inut
                background #af99d4
        .agreement
            height 21px
        .reg_form_item_name
            width 60px
        .reg_form_item_input
            width 150px
        .validate_num_input
            height 30px
            vertical-align top
            padding-top 3px
        #txtValidate
            width 50px
        #validateimg
            width 90px
            margin-left 5px
            height 27px
            vertical-align top
            cursor pointer
        #reg_step2,#reg_step3,#reg_step4,#reg_step5
            width 220px
            margin 0 auto
            padding-top 30px
            line-height 25px
            display none
            height 120px
            text-align center
            input
                background #776595
                border 1px solid #683ec8
                height 18px
                color #221334
                line-height 18px
                width 145px
        #reg_step2
            height auto
            .active
                height 200px
                line-height 30px
        #active_link
            display block
            margin 20px auto
            width 106px
            height 30px
            background url($reg_btn) no-repeat -8px -63px
            &:hover
                background-position -8px -93px
        .download
            height 25px
            width 210px
            position absolute
            top 330px
            left 30px
            .mini_download
                height 25px
                width 103px
                display block
                float left
                background url($reg_btn) no-repeat -8px -5px
                &:hover
                    background-position:-8px -31px
            .full_download
                height 25px
                width 103px
                display block
                float right
                background url($reg_btn) no-repeat -114px -5px
                &:hover
                    background-position:-114px -31px
        .reg_submit
            height 55px
            position relative
        #reg_button
            width 106px
            height 30px
            position absolute
            left 61px
            top 7px
            background url($reg_btn) no-repeat -118px -63px
            &:hover
                background-position -118px -93px
        .icon
            width 70px
            height 70px
            background url($reg_btn) no-repeat  - 718px -12px
            margin 10px auto
            a
                display none
        .reg_success
            background url($reg_btn) no-repeat -280px 0
            width 145px
            height 30px
            margin 0 auto
        .act_success
            background url($reg_btn) no-repeat -277px -64px
            width 145px
            height 30px
            margin 0 auto
        .act_fail
            background url($reg_btn) no-repeat -450px 0
            width 145px
            height 30px
            margin 0 auto
        #back
            display block
            text-decoration underline
            cursor pointer
            margin 20px auto
        .check_result_wrong
            background url($right_wrong) no-repeat 0 -32px
            height 100%
            width 16px
        .check_result_right 
            background: url($right_wrong) no-repeat 0 4px
            height: 25px;
            width: 16px;

        .check_result_loading 
            background url($loader) no-repeat 0 4px
            height 25px
            width 16px
    #close
        width 22px
        height 22px
        cursor pointer
        position absolute
        top 10px
        right 10px
.disnone
    display none
