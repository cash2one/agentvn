            top 40px
            left 81px
            background url($reg_btn) no-repeat -267px -125px
            &:hover
                background-position -267px -157px
        .check_result_wrong
            background url($right_wrong) no-repeat 0 -32px
            height 100%
            width 16px
        .check_result_right 
            background: url($right_wrong) no-repeat 0 4px
            height: 25px;
            width: 16px;

        .check_result_loading 
            background url($loader) no-repeat 0 4px
            height 25px
            width 16px
    #close
        width 22px
