            &:hover
                background-position -2px -93px
        .download
            height 66px
            padding 0 10px
            position relative
            margin 0 auto
            background url($download) no-repeat
            .mini_download
                height 30px
                width 125px
                display block
                position relative
                top 80px
                float left
                background url($reg_btn) no-repeat -13px -125px
