        .reg_submit
            height 55px
            position relative
        #reg_button
            width 120px
            height 30px
            position absolute
            left 51px
            top 11px
            background url($reg_btn) no-repeat -124px -61px
            &:hover
                background-position -124px -93px
        .success
            background url($success) no-repeat
            padding 10px 40px
            display inline
            color #FAAB10
        .fail
            background url($fail) no-repeat
            padding 10px 40px
            display inline
            color #FAAB10
        .back_bg
            position absolute
            top 360px
            height 20px
            width 100%
            background url($download) no-repeat
